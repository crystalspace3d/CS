<shader compiler="xmlshader" name="fx">
  <!--<shadervars>
    <shadervar name="option" type="float">0.0</shadervar>
  </shadervars>-->
  <technique priority="200">
    <pass>
      <buffer source="texture coordinate 0" destination="texture coordinate 0" />
      <!-- vp plugin="glfixed" / -->
      <texture destination="Texture1" name="tex diffuse" />
	  <texture destination="DiffuseTex" name="tex gbuffer 0" />
	  <texture destination="NormalTex" name="tex gbuffer 1" />
	  <texture destination="AmbientTex" name="tex gbuffer 2" />
	  <texture destination="DepthTex" name="tex gbuffer depth" />
      <fp plugin="glcg" file="/shader/postproc/fx.cgfp" />
    </pass>
  </technique>
</shader>
