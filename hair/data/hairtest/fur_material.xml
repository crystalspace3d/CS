<?xml version='1.0' encoding='UTF-8'?>
<library>
  <shaders>
    <shader>
      <file>/shader/fur/marschner_lookup_cg.xml</file>
    </shader>		
    <shader>
      <file>/shader/lighting/lighting_default_shadowmap.xml</file>
    </shader>		
  </shaders>
  <textures>
    <texture name='krystal_skull_diffusemap'>
      <file>/lib/krystal/krystal_skull_diffusemap.png</file>
    </texture>	
    <texture name='krystal_skull_colormap'>
      <file>/lib/krystal/krystal_skull_colormap.png</file>
    </texture>	
    <texture name='krystal_skull_texturemap'>
      <file>/lib/krystal/krystal_skull_texturemap.png</file>
    </texture>	
  </textures>
  <materials>
    <material name='marschner_material'>
      <shader type='standard'>marschner_lookup_cg</shader>

      <shader type='shadow'>lighting_default_shadowmap</shader>
	  
	  <shadervar type="int" name="diffuse type">4</shadervar>
	  <shadervar type="vector3" name="hair color">0.51, 0.34, 0.25</shadervar>
	  <shadervar type="texture" name="texture map">krystal_skull_texturemap</shadervar>
	  <shadervar type="texture" name="color map">krystal_skull_colormap</shadervar> 
    </material>
  </materials>
  <sounds/>
</library>
